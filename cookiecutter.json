{
  "author": "Richard Wen",
  "email": "rrwen.dev@gmail.com",
  "template_name": "template_name",
  "template_description": "template_description",
  "github_user": "rrwen",
  "github_url": "{{'https://github.com/' + cookiecutter.github_user + '/' + cookiecutter.template_name}}",
  "github_short": "{{cookiecutter.github_user + '/' + cookiecutter.template_name}}",
  "tex_firstname": "{{cookiecutter.author.split(' ')[0]}}",
  "tex_lastname": "{{cookiecutter.author.split(' ')[-1]}}",
  "tex_affil": "Department of Civil Engineering, Ryerson University, Toronto, ON",
  "tex_year": "{% now 'utc', '%Y' %}",
  "tex_title": "tex_title",
  "tex_short": "{{cookiecutter.template_name.replace(' ', '_').replace('-','_')}}",
  "tex_file": "cookiecutter.tex_firstname + '_' + {{(cookiecutter.tex_lastname + '_' + cookiecutter.tex_year + '_' + cookiecutter.tex_short).lower()}}",
  "tex_documentclass": "[letterpaper,12pt]{article}",
  "tex_setmainfont": "{Times New Roman}",
  "tex_geometry": "{margin=1in}",
  "tex_graphicspath": "{% raw %}{{./img/}}{% endraw %}",
  "tex_DeclareGraphicsExtensions": "{.pdf,.png}"
}
